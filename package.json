{
  "name": "incognito-monitor",
  "productName": "Incognito Monitor",
  "author": {
    "name": "Incognito",
    "email": "incognito@autonomous.nyc"
  },
  "version": "1.3.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/incognitochain/incognito-monitor.git"
  },
  "main": "./electron/build.js",
  "build": {
    "appId": "incognito.monitoor",
    "mac": {
      "category": "incognito"
    },
    "win": {
      "target": "portable"
    },
    "linux": {
      "target": "AppImage"
    },
    "files": [
      "!node_modules/**/*",
      "build/**/*",
      "electron/build.js",
      "electron/update.html"
    ],
    "extraResources": [
      {
        "from": "./data.sample",
        "to": "./data.sample"
      },
      {
        "from": "./update.html",
        "to": "./update.html"
      },
      {
        "from": "./run.sh",
        "to": "./run.sh"
      },
      {
        "from": "./icon.png",
        "to": "./icon.png"
      },
      {
        "from": "./icon.icns",
        "to": "./icon.icns"
      }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "incognitochain",
        "repo": "incognito-monitor"
      }
    ]
  },
  "homepage": "./",
  "dependencies": {
    "@blueprintjs/core": "^3.17.1",
    "@blueprintjs/select": "^3.9.0",
    "@blueprintjs/table": "^3.6.0",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-promise": "^0.5.28",
    "@types/uuid": "^3.4.5",
    "electron-is-dev": "^1.1.0",
    "fix-path": "^2.1.0",
    "immutable": "^4.0.0-rc.12",
    "jayson": "^3.0.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "shelljs": "^0.8.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0"
  },
  "scripts": {
    "start-react": "node scripts/start.js",
    "build-react": "node scripts/build.js",
    "build-electron": "webpack --config ./config/webpack.electron.config.js",
    "test": "node scripts/test.js",
    "electron": "electron ./electron",
    "package": "rimraf -rf dist/ && electron-builder -mwl",
    "release": "yarn build-react && electron-builder --publish=always",
    "build": "yarn build-electron && yarn build-react && yarn package",
    "publish": "build -mwl -p always",
    "start": "concurrently \"cross-env BROWSER=none yarn start-react\" \"wait-on http://localhost:3000 && electron .\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@svgr/webpack": "4.1.0",
    "@types/immutable": "^3.8.7",
    "@types/jest": "24.0.15",
    "@types/lodash": "^4.14.136",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.0",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "2.1.1",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "electron": "^15.5.5",
    "electron-builder": "^20.44.4",
    "electron-log": "^3.0.6",
    "electron-updater": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jest": "24.7.1",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "0.3.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "react-dev-utils": "^9.0.1",
    "resolve": "1.10.0",
    "rimraf": "^2.6.3",
    "sass-loader": "7.1.0",
    "semver": "6.0.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "ts-pnp": "1.1.2",
    "typescript": "3.5.3",
    "url-loader": "1.1.2",
    "wait-on": "^3.2.0",
    "webpack": "4.29.6",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.2.0"
  },
  "license": "MIT"
}
